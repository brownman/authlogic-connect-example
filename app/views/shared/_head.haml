%meta{:content => "text/html; charset=UTF-8", "http-equiv" => "content-type"}/
%meta{:content => "", :name => "description"}/
= yield :meta_head

%link{:href => "/stylesheets/reset.css", :media => "screen", :rel => "stylesheet", :type => "text/css"}/
%link{:href => "/stylesheets/application.css", :rel => "stylesheet", :type => "text/css"}/
= yield :stylesheet_head

- %w(jquery/jquery-1.4.2 jquery/jquery.rails).each do |javascript|
  = javascript_include_tag "http://cachedcommons.org/javascripts/#{javascript}-min.js"
- %w(jquery.jcarousel.pack cufon.yui kievit).each do |javascript|
  = javascript_include_tag "/javascripts/#{javascript}.js"

= javascript_include_tag "application"

= yield :javascript_head

= yield :extra_head